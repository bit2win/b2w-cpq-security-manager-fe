    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build": "npm run build-lib",
    "build-lib": "tsc && npm run copy-css-to-lib",
    "start": "webpack serve --config webpack.dev.js --port 3000 --progress --hot --inline --history-api-fallback --content-base target -o target/[name].[contenthash].js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "serve:spa": "http-server -p 3028 --cors=* target_spa",
    "watch:spa": "webpack --config webpack.spa.dev.js --watch",
    "build:spa:dev": "webpack --config webpack.spa.dev.js",
    "build:spa:prod": "webpack --config webpack.spa.prod.js",
    "bump:alpha": "npm i @bit2win/b2w-design-system@alpha && npm i @bit2win/b2w-design-system-react@alpha",
    "bump:core-lib": "npm i @bit2win/b2w-core-lib@alpha",
    "bump:design-system": "npm i @bit2win/b2w-design-system@alpha && npm i @bit2win/b2w-design-system-react@alpha",
    "artifactregistry-login": "npx google-artifactregistry-auth"
  },
